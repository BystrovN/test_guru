<h1><%= t('.header', title: @test_passage.test.title) %></h1>

<p>
  <% if @test_passage.success_percentage >= TestPassage::SUCCESS_PERCENTAGE %>
    <span style="color: green;"><%= @test_passage.success_percentage %> <%= t('.pass') %></span>
  <% else %>
    <span style="color: red;"><%= @test_passage.success_percentage %> <%= t('.failed') %></span>
  <% end %>
</p>

<%= link_to t('.all_tests'), tests_path %>
