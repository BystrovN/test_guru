<tr>
  <td style="text-align: center;"><%= test.title %></td>
  <td style="text-align: center;"><%= test.level %></td>
  <td style="text-align: center;"><%= test.questions.count %></td>
  <td style="text-align: center;">
    <%= link_to t('links.show'), admin_test_path(test) %>
  </td>
  <td style="text-align: center;">
    <%= link_to t('links.edit'), edit_admin_test_path(test) %>
  </td>
  <td style="text-align: center;">
    <%= link_to t('links.delete'), admin_test_path(test), method: :delete, data: { confirm: t('actions.sure?') } %>
  </td>
</tr>
